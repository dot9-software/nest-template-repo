{
  "name": "nest-template-repo",
  "version": "1.0.0",
  "main": "index.js",
  "author": "dot9 GmbH <contact@dot9.co>",
  "scripts": {
    "prebuild": "rimraf dist",
    "inittestdb": "./scripts/init-test-db.sh",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "test": "yarn inittestdb && jest --clearCache && jest",
    "test:fixtures": "yarn run-fixtures && jest --clearCache && jest",
    "test:watch": "yarn inittestdb && jest --clearCache && jest --watch",
    "test:cov": "yarn inittestdb && jest --clearCache && jest --coverage",
    "test:debug": "yarn inittestdb && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --clearCache --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -d ormconfig.ts",
    "db:diagram": "ts-node ./node_modules/typeorm-uml/bin/run ormconfig.ts --with-entity-names-only -d=ER-Diagram.png",
    "run-fixtures": "DATABASE_URL=postgresql://postgres:docker@localhost:8888/trtestdb yarn fixtures ./test/fixtures --config ./tests.ormconfig.ts --sync --require=ts-node/register --require=tsconfig-paths/register -d",
    "dummy-fixtures": "./scripts/dummy-fixtures.sh"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "yarn": "1.x",
    "node": ">=16.x"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.5",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.2",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^6.17.9",
    "@sentry/tracing": "^6.17.9",
    "@types/morgan": "^1.9.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "dayjs": "^1.11.0",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nest-morgan": "^2.0.0",
    "nest-optional-validation-pipes": "^1.0.5",
    "passport": "^0.5.2",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rrule": "^2.7.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typeorm-fixtures-cli": "^1.10.1",
    "typeorm-uml": "^1.6.5",
    "typescript": "^4.3.5"
  }
}
